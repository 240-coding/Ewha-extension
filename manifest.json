{
    "manifest_version": 2,
    "name": "이화 바로가기",
    "version": "1.0.0",
    "description": "이화여대 학생들을 위한 바로가기 모음",
    "icons": {
        "16": "./images/logo.png",
        "48": "./images/logo.png",
        "128": "./images/logo.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "./images/toolbar-logo.png"
        },
        "default_title": "이화 바로가기",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com/6478f529f2.js; object-src 'self'"
}